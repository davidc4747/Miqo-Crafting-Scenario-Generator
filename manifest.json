{
    "name": "Miqo Crafting Scenario Generator",
    "version": "0.3.0",
    "description": "Creates miqo crafting scenarios",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://ffxivcrafting.com/crafting/from-list*",
                "https://ffxivcrafting.com/crafting/by-class*"
            ],
            "js": [
                "jquery.min.js",
                "clipboard.min.js",
                "miqo-crafting-scenario-generator.js"
            ]
        },
        {
            "matches": ["https://ffxivcrafting.com/recipes*"],
            "js": ["add-multi-button.js"],
            "css": ["add-multi-button..css"]
        }
    ]
}
